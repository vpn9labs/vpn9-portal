# Kamal secrets configuration
# Copy this file to .kamal/secrets and fill in your actual values

# Docker registry password
KAMAL_REGISTRY_PASSWORD=your-registry-password

# Rails encryption key (from config/credentials.yml.enc)
RAILS_MASTER_KEY=your-rails-master-key

# PostgreSQL credentials
POSTGRES_USER=vpn9_production
POSTGRES_PASSWORD=secure-password-here

# Database URL (update with your actual credentials)
DATABASE_URL=postgresql://vpn9_production:secure-password-here@vpn9-db:5432/vpn9_production

# Payment processor configuration
PAYMENT_PROCESSOR=bitcart

# Bitcart configuration
BITCART_BASE_URL=https://your-bitcart-instance.com
BITCART_API_KEY=your-bitcart-api-key